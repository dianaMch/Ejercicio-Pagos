{
  "openapi": "3.0.1",
  "info": {
    "title": "PAGO.API",
    "version": "1.0"
  },
  "paths": {
    "/api/auth": {
      "post": {
        "tags": [
          "Auth"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/LoginDataInput"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/LoginDataInput"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/LoginDataInput"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/pagos": {
      "post": {
        "tags": [
          "Pago"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/PostDataInput"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/PostDataInput"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/PostDataInput"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      },
      "put": {
        "tags": [
          "Pago"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/PutPagoDataInput"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/PutPagoDataInput"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/PutPagoDataInput"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/pagos/{id}": {
      "get": {
        "tags": [
          "Pago"
        ],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "LoginDataInput": {
        "type": "object",
        "properties": {
          "usuario": {
            "type": "string",
            "nullable": true
          },
          "contrase√±a": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "PostDataInput": {
        "type": "object",
        "properties": {
          "remitente": {
            "type": "integer",
            "format": "int32"
          },
          "destinatario": {
            "type": "integer",
            "format": "int32"
          },
          "productos": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Productos"
            },
            "nullable": true
          },
          "totalProductos": {
            "type": "integer",
            "format": "int32"
          },
          "montoTotal": {
            "type": "number",
            "format": "double"
          },
          "concepto": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "Productos": {
        "type": "object",
        "properties": {
          "idProducto": {
            "type": "integer",
            "format": "int32"
          },
          "cantidad": {
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      },
      "PutPagoDataInput": {
        "required": [
          "estado",
          "idPago"
        ],
        "type": "object",
        "properties": {
          "idPago": {
            "type": "integer",
            "format": "int32"
          },
          "estado": {
            "minLength": 1,
            "type": "string"
          }
        },
        "additionalProperties": false
      }
    }
  }
}